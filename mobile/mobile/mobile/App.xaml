<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="mobile.App">
    <Application.Resources>
        <ResourceDictionary>
            <OnPlatform x:TypeArguments="x:String" 
                x:Key="FontAwesomeBrands">
                <On Platform="Android" 
          Value="FontAwesome5Regular400.otf#Regular" />
                <On Platform="iOS" 
          Value="FontAwesome5Regular400-Regular" />
            </OnPlatform>

            <OnPlatform x:TypeArguments="x:String" 
                x:Key="FontAwesomeSolid">
                <On Platform="Android" 
          Value="FontAwesomeRegular400.otf#Regular" />
                <On Platform="iOS" 
          Value="FontAwesomeRegular400-Regular" />
            </OnPlatform>

            <OnPlatform x:TypeArguments="x:String" 
                x:Key="FontAwesomeRegular">
                <On Platform="Android" 
          Value="FontAwesomeSolid900.otf#Regular" />
                <On Platform="iOS" 
          Value="FontAwesomeSolid900-Solid" />
            </OnPlatform>
        </ResourceDictionary>
    </Application.Resources>
</Application>